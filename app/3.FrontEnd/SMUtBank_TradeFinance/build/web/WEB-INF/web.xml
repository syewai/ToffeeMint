        <url-pattern>/issuingBank/lcDetails</url-pattern>
    </servlet-mapping>
    
     <!---View BOL/Insurance/Cert of Origin-->
   
   
   
    
    <!--Advising Bank Features-->
    <!---home-->
    <servlet>
        <servlet-name>advisingBankHome</servlet-name>
        <jsp-file>/advisingBank/home.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>advisingBankHome</servlet-name>
        <url-pattern>/advisingBank/</url-pattern>
    </servlet-mapping>

     <!---View LC Log-->
    <servlet>
        <servlet-name>advisingBankViewLcLog</servlet-name>
        <jsp-file>/advisingBank/viewLcLog.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>advisingBankViewLcLog</servlet-name>
        <url-pattern>/advisingBank/viewLcLog</url-pattern>
    </servlet-mapping>
    
    <!---LC Details-->
    <!---Approve LC request- Same page as view LC Details-->
    <servlet>
        <servlet-name>advisingBankLcDetails</servlet-name>
        <jsp-file>/advisingBank/lcDetails.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>advisingBankLcDetails</servlet-name>
        <url-pattern>/advisingBank/lcDetails</url-pattern>
    </servlet-mapping>
    
     <!---View BOL/Insurance/Cert of Origin-->
 
 
 
    <!---Shipper Features-->
    <!--Home-->
    <servlet>
        <servlet-name>shipperHome</servlet-name>
        <jsp-file>/shipper/home.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>shipperHome</servlet-name>
        <url-pattern>/shipper/</url-pattern>
    </servlet-mapping>
    
    <!--View Shipping Log-->
    <servlet>
        <servlet-name>shipperViewShippingLog</servlet-name>
        <jsp-file>/shipper/ViewShippingLog.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>shipperViewShippingLog</servlet-name>
        <url-pattern>/shipper/viewShippingLog</url-pattern>
    </servlet-mapping>
    
    <!--shipping Details-->
    <!--Submit Bill of Ladding - Same page as shipping details-->
    <!--Verify goods location - Same page as shipping details-->
    <servlet>
        <servlet-name>shipperShippingDetails</servlet-name>
        <jsp-file>/shipper/shippingDetails.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>shipperShippingDetails</servlet-name>
        <url-pattern>/shipper/shippingDetails</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
  
</web-app>

