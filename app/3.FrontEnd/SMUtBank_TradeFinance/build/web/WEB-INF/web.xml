<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>CorsFilter</filter-name>
        <filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CorsFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <!---Login-->
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/login.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    <!--Importer Features-->
    
    <!---Importer Home-->
    <servlet>
        <servlet-name>importerHome</servlet-name>
        <jsp-file>/importer/importer.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>importerHome</servlet-name>
        <url-pattern>/importer/</url-pattern>
    </servlet-mapping>
    
    <!---Apply LC-->
    <servlet>
        <servlet-name>importerApplyLc</servlet-name>
        <jsp-file>/importer/applyLc.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>importerApplyLc</servlet-name>
        <url-pattern>/importer/applyLc</url-pattern>
    </servlet-mapping>
    
     <!---View LC Log-->
    <servlet>
        <servlet-name>importerViewLcLog</servlet-name>
        <jsp-file>/importer/viewLcLog.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>importerViewLcLog</servlet-name>
        <url-pattern>/importer/viewLcLog</url-pattern>
    </servlet-mapping>
    
    <!---LC Details-->
    <!---Amend LC request -same page as view LC details-->
    <servlet>
        <servlet-name>importerLcDetails</servlet-name>
        <jsp-file>/importer/lcDetails.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>importerLcDetails</servlet-name>
        <url-pattern>/importer/lcDetails</url-pattern>
    </servlet-mapping>
        
    <!---Apply Trust Receipt-->
    <servlet>
        <servlet-name>importerApplyTr</servlet-name>
        <jsp-file>/importer/applyTr.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>importerApplyTr</servlet-name>
        <url-pattern>/importer/applyTr</url-pattern>
    </servlet-mapping>
    
    
  <!--Exporter Features-->
    <!---Home-->
    <servlet>
        <servlet-name>exporterHome</servlet-name>
        <jsp-file>/exporter/exporter.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>exporterHome</servlet-name>
        <url-pattern>/exporter/</url-pattern>
    </servlet-mapping>
    
    <!---View LC Log-->
    <servlet>
        <servlet-name>exporterViewLcLog</servlet-name>
        <jsp-file>/exporter/viewLcLog.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>exporterViewLcLog</servlet-name>
        <url-pattern>/exporter/viewLcLog</url-pattern>
    </servlet-mapping>
    
    <!---LC Details-->
    <!---Approve LC - same page as view lc details-->
    <servlet>
        <servlet-name>exporterLcDetails</servlet-name>
        <jsp-file>/exporter/lcDetails.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>exporterLcDetails</servlet-name>
        <url-pattern>/exporter/lcDetails</url-pattern>
    </servlet-mapping>
    
    <!--Ship Goods-->
    <servlet>
        <servlet-name>exporterShipGoods</servlet-name>
        <jsp-file>/exporter/shipGoods.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>exporterShipGoods</servlet-name>
        <url-pattern>/exporter/shipGoods</url-pattern>
    </servlet-mapping>
    
    <!--Request Banker's Guarantee-->
    <servlet>
        <servlet-name>exporterRequestGuarantee</servlet-name>
        <jsp-file>/exporter/requestGuarantee.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>exporterRequestGuarantee</servlet-name>
        <url-pattern>/exporter/requestGuarantee</url-pattern>
    </servlet-mapping>
    
    <!---View BOL/Insurance/Cert of Origin-->
   
   
   
    
    <!--Issuing Bank Features-->
    <!---home-->
    <servlet>
        <servlet-name>issuingBankHome</servlet-name>
        <jsp-file>/issuingBank/home.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>issuingBankHome</servlet-name>
        <url-pattern>/issuingBank/</url-pattern>
    </servlet-mapping>

     <!---View LC Log-->
